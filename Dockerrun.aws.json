{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "flask-app",
      "image": "alekcoraglez/flask-app-docker",
      "memoryReservation": 512,
      "cpu": 1,
      "hostname": "flask-app",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ]
    }
  ]
}
